<form name="editform" action="userEdit" method="post" accept-charset="utf-8">
        <h1>Account - Edit</h1>
        <input type="hidden" name="address" value="<%=data[0].address  %>" />
        <div>
            <label>name：</label>
            <input type="text" name="name" placeholder="name" value="<%=data[0].name %>"  />
        </div>
        <div>
            <label>abi：</label>
            <input type="text" name="abi" placeholder="abi" value="<%=data[0].abi %>" />
        </div>
        <div>
            <label>contract：</label>
            <input type="text" name="contract" placeholder="contract" value="<%=data[0].contract %>">
        </div>
        <div>
            <input type='submit' value='Submit'>
            <input type='reset' value='Reset'>
            <input type='button' value='Back' onclick="javascript: window.history.back();">
        </div>
    </form>
